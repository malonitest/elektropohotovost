		{ "route": "/zapadni-praha/praha-6", "redirect": "/zapadni-praha/praha-6/", "statusCode": 301 },
		{ "route": "/zapadni-praha/dejvice", "redirect": "/zapadni-praha/dejvice/", "statusCode": 301 },
		{ "route": "/zapadni-praha/brevnov", "redirect": "/zapadni-praha/brevnov/", "statusCode": 301 },

