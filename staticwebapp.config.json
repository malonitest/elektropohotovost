		{ "route": "/praha-zapad/rudna", "redirect": "/praha-zapad/rudna/", "statusCode": 301 },
		{ "route": "/praha-zapad/jesenice", "redirect": "/praha-zapad/jesenice/", "statusCode": 301 },
